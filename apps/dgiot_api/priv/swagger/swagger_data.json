      "post": {
        "summary": "添加物模型",
        "description": "添加物模型",
        "parameters": [
          {
            "in": "body",
            "name": "data",
            "description": "关联信息",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "productid": {
                  "description": "产品标识",
                  "type": "string",
                  "example": "ed09e37bbb"
                },
                "item": {
                  "schema": {
                    "description": "物模型单项指标",
                    "properties": {
                      "name": {
                        "description": "物模型功能名称",
                        "type": "string",
                        "example": "电压"
                      },
                      "identifier": {
                        "description": "物模型标识符",
                        "type": "string",
                        "example": "voltage"
                      },
                      "accessMode": {
                        "description": "读写类型",
                        "type": "string",
                        "example": "r",
                        "enum": [
                          "r",
                          "rw"
                        ]
                      },
                      "devicetype": {
                        "description": "物模型类型",
                        "type": "string",
                        "example": "控制器"
                      },
                      "index": {
                        "description": "序号",
                        "type": "number",
                        "example": 1
                      },
                      "isshow": {
                        "description": "是否显示",
                        "type": "boolean",
                        "example": true
                      },
                      "isstorage": {
                        "description": "是否存储",
                        "type": "boolean",
                        "example": true
                      },
                      "moduleType": {
                        "description": "类型",
                        "type": "string",
                        "example": "properties"
                      },
                      "dataForm": {
                        "schema": {
                          "description": "物模型数据采集指标",
                          "properties": {
                            "collection": {
                              "description": "采集公式，%s为占位符，表示自身值",
                              "type": "string",
                              "example": "%s"
                            },
                            "control": {
                              "description": "控制下发公式，%d为占位符，表示自身值",
                              "type": "string",
                              "example": "%d"
                            },
                            "protocol": {
                              "description": "协议类型（动态获取）",
                              "type": "string",
                              "example": "MODBUSRTU"
                            },
                            "order": {
                              "description": "采集顺序",
                              "type": "number",
                              "example": 0
                            },
                            "round": {
                              "description": "采集轮次",
                              "type": "string",
                              "example": "all",
                              "enum": [
                                "all",
                                "first",
                                "last"
                              ]
                            },
                            "strategy": {
                              "description": "采集策略",
                              "type": "string",
                              "example": "采集值",
                              "enum": [
                                "采集值",
                                "计算值",
                                "主动上报"
                              ]
                            }
                          }
                        },
                        "required": false,
                        "type": "object"
                      },
                      "dataType": {
                        "schema": {
                          "description": "物模型数据存储",
                          "properties": {
                            "type": {
                              "description": "类型",
                              "type": "string",
                              "example": "int",
                              "enum": [
                                "int",
                                "long",
                                "float",
                                "double",
                                "text",
                                "boole",
                                "date",
                                "enum",
                                "object"
                              ]
                            },
                            "specs": {
                              "schema": {
                                "description": "数据存储",
                                "properties": {
                                  "min": {
                                    "description": "取值范围(最小值)",
                                    "type": "number",
                                    "example": -99999
                                  },
                                  "max": {
                                    "description": "取值范围(最大值)",
                                    "type": "number",
                                    "example": 99999
                                  },
                                  "precision": {
                                    "description": "小数位数",
                                    "type": "number",
                                    "example": 3
                                  },
                                  "step": {
                                    "description": "步长",
                                    "type": "number",
                                    "example": 3
                                  },
                                  "unit": {
                                    "description": "单位",
                                    "type": "string",
                                    "example": "%"
                                  }
                                }
                              },
                              "required": false,
                              "type": "object"
                            }
                          }
                        },
                        "required": false,
                        "type": "object"
                      },
                      "dataSource": {
                        "schema": {
                          "description": "物模型协议数据（动态）",
                          "properties": {
                            "_dlinkindex": {
                              "description": "报文序号",
                              "type": "number",
                              "example": 1
                            }
                          }
                        }
                      }
                    },
                    "required": true,
                    "type": "object"
                  }
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns success"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "tags": [
          "Product"
        ]
      },
