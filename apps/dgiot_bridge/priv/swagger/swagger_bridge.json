    "/uniapp/{instruct}/{deviceid}": {
      "put": {
        "summary": "下发指令",
        "description": "下发指令",
        "parameters": [
          {
            "in": "path",
            "name": "instruct",
            "description": "指令",
            "required": true,
            "type": "string",
            "enum": [
              "photo",
              "scancode"
            ]
          },
          {
            "in": "path",
            "name": "deviceid",
            "description": "设备id",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "data",
            "description": "下发值",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "other": {
                  "type": "string",
                  "description": "自定义",
                  "example": "拍照"
                }
              }
